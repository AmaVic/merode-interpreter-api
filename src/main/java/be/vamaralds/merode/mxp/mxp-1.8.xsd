    <xs:element name="edgecontrolpoints">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="mxp:edgecontrolpoint"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="edgecontrolpoint">
        <xs:complexType>
            <xs:attribute name="value" type="xs:string" use="required"/>
        </xs:complexType>
